{
    "index": "/index.html",
    "assetGroups": [{
        "name": "app",
        "installMode": "prefetch",
        "resources": {
            "files": [
                "/favicon.ico",
                "/index.html",
                "/manifest.json",
                "/*.css",
                "/*.js"
            ]
        }
    }, {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
            "files": [
                "/assets/**",
                "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)",
                "!/assets/material-design-icons/**"
            ]
        }
    }, {
        "name": "fonts",
        "resources": {
            "urls": [
                "https://fonts.googleapis.com/**",
                "https://fonts.gstatic.com/**"
            ]
        }
    }],
    "dataGroups": [{
            "name": "api-freshness",
            "urls": [
                "api/news"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "3d",
                "timeout": "1m",
                "strategy": "freshness"
            }
        },
        {
            "name": "api-performance",
            "urls": [
                "api/list"
            ],
            "cacheConfig": {
                "maxSize": 100,
                "maxAge": "10d",
                "timeout": "10s",
                "strategy": "performance"
            }
        }
    ]
}
